# Snakemake Workflow for Module 6: Running RaceID / StemID on normalized combined count tables

# Include common settings
include: "../common.snakefile"

rule combine-plates:
    input:
        countscombined = '{output}/combine_plates/{reference}/{fileprefix}/counttables_combined.tsv'
    output:
        stemraceidreport = '{output}/RaceID2_StemID_results.html'
        clustertables = '{output}/cell_clust.xlx'
    script:
        'RaceID2_StemID_sample.R'